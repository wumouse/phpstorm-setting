<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_TRAILING_WHITESPACES_SHOWN" value="false" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\btodo:haow1\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="51bff" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\btodo:wumouse\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="51bff" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <filter name="mine">
      <pattern index="3" />
      <pattern index="2" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>